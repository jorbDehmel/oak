package!("std");
use_rule!("std");
include!("stl/set.oak");

let hash(what: i32) -> u128
{
    if what == 0u128
    {
        1u128
    }
    to_u128(what)
}

let main() -> i32
{
    let s: stl::set<i32>;

    s.insert(1i32);
    s.insert(3i32);
    s.insert(5i32);

    for (let i = 0i32; i < 6i32; ++i)
    {
        if s.has(i)
        {
            print(i);
            endl();
        }
    }

    s.erase(3i32);

    print("After erasure:\n");
    for (let i = 0i32; i < 6i32; ++i)
    {
        if s.has(i)
        {
            print(i);
            endl();
        }
    }

    0i32
}
