package!("std");
include!("stl/arr.oak");
use_rule!("std");

let size! = 20i32;

let main() -> i32
{
    let a: stl::arr<i32>;

    a = size!;

    let x: u128 = 0u128;
    while x < size!
    {
        a.set(x, to_i32(x));

        ++x;
    }

    x = 0u128;
    while x < size!
    {
        print(a.Get(x));
        print(" ");

        ++x;
    }

    endl();

    0i32
}
