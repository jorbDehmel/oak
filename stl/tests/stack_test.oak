package!("std");
use_rule!("std");
include!("stl/stack.oak");

let main() -> i32
{
    let q: stl::stack<i32>;

    print("Pushing\n");
    for (let i = 0i32; i < 10i32; ++i)
    {
        print(i);
        endl();
        q.push(i);
    }

    print("Popping\n");
    while (Not(q.empty()))
    {
        let res = q.top();
        let i: i32 = res.unwrap();

        print(i);
        endl();

        q.pop();
    }

    0i32
}
