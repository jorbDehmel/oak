package!("std");
include!("stl/matrix.oak");
use_rule!("std");

let main() -> i32
{
    let m: stl::matrix<i32>;

    m = (3i32, 3i32);

    let y: u128 = 0u128;
    while y < 3u128
    {
        let x = 0u128;
        while x < 3u128
        {
            m.set(x, y, to_i32(x * y));
            ++x;
        }
        ++y;
    }

    y = 0u128;
    while y < 3u128
    {
        let x = 0u128;
        while x < 3u128
        {
            print(m.Get(x, y));
            print(" ");
            ++x;
        }

        endl();
        ++y;
    }

    print("Row test:\n");

    let r = m.row(1u128);
    let i = 0u128;
    while i < r.l
    {
        print(r.Get(i));
        print(" ");
        ++i;
    }
    endl();

    0i32
}
