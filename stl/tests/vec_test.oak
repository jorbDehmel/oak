package!("std");
use_rule!("std");

include!("stl/vec.oak");

let main() -> i32
{
    // Init new vec
    let obj: stl::vec<i32>;

    let i = 0i32;
    while (i < 100i32)
    {
        obj.push_back(i);
        ++i;
    }

    i = 0i32;
    while (i < 110i32)
    {
        let result: opt<^i32>;
        result = obj.Get(to_i128(i));

        print(i);
        print("\t");

        match (result)
        {
            case some(data)
            {
                print(^^data);
                print("\n");
            }

            case none()
            {
                print("Error: out of range!\n");
            }
        }

        ++i;
    }

    0i32
}
