package!("std");
include!("extra/bit_print.oak", "extra/bitset.oak");
use_rule!("std");

let println(what: bool) -> void
{
    if what
    {
        print("true\n");
    }
    else
    {
        print("false\n");
    }
}

let main() -> i32
{
    println<^extra::bitset<64>>(_: ^extra::bitset<64>);

    let b: extra::bitset<64>;

    let c = b.Get(0u128);
    let res = c.unwrap();
    println(b);

    b.flip(0u128);

    c = b.Get(0u128);
    res = c.unwrap();
    println(b);

    b.clear(0u128);

    0i32
}
