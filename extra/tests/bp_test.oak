// Tests the bit print functions from the extra package

package!("std");
use_rule!("std");
include!("extra/bit_print.oak");

let main() -> i32
{
    let j = 0u8;
    while j < 255u8
    {
        extra::bit_print(@j);
        print("\n");

        extra::hex_print(@j);
        print("\n");

        ++j;
    }

    0i32
}
