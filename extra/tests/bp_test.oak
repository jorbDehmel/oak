// Tests the bit print functions from the extra package

package!("std");
include!("extra/bit_print.oak");

let main() -> i32
{
    let j: u8;
    extra::bit_print<u8>(_: ^u8);
    extra::hex_print<u8>(_: ^u8);

    j = 0;
    while (j < to_u8(255))
    {
        extra::bit_print(j);
        print("\n");

        extra::hex_print(j);
        print("\n");

        j += 1;
    }

    0
}
