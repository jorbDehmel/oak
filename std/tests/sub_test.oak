package!("std");
include!("std/io.oak");
include!("std/panic.oak");

let main() -> i32
{
    let i: i32;
    i = (5i32 * 6i32 / 2i32) + (15i32 / 3i32);
    println(i); // 20

    assert!(i == 20i32);

    i = 5i32 * 6i32 / 2i32 + 15i32 / 3i32;
    println(i); // 20

    assert!(i == 20i32);

    0i32
}
