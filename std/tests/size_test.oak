package!("std");
use_rule!("std");

let main() -> i32
{
    let a: i8;

    print(size!(a));
    print("\n");

    let b: i16;

    print(size!(b));
    print("\n");

    let c: i32;

    print(size!(c));
    print("\n");

    let d: i64;

    print(size!(d));
    print("\n");

    let e: ^i64;

    print(size!(e));
    print("\n");

    0
}
