package!("std");
use_rule!("std");

let main() -> i32
{
    let item_a: i32;
    let item_b: type!(item_a);

    item_a = 123i32;
    item_b = item_a;

    print(item_a);
    print("\n");
    print(item_b);
    print("\n");

    0i32
}
