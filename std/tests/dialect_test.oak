/*
A demonstration using the standard Oak dialect
*/

package!("std");
use_rule!("std");

let obj: struct
{
    a: i32,
}

let obj.Copy(what: i32) -> obj
{
    self.a = what;
    ^self
}

let obj.method() -> void
{
    print(self.a);
    print("\n");
}

let main() -> i32
{
    print("Hello, there!\n");

    let inst: obj = 5i32;
    inst.method();

    0i32
}
