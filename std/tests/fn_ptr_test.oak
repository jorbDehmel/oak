// A test of Oak's function pointers.
// Passed Aug 8, 23

package!("std");

let a(what: i32) -> i32
{
    what
}

let b(what: i32) -> i32
{
    (2i32 * what)
}

let main() -> i32
{
    let f: ^(i32) -> i32;
    ptrcpy!(f, @a);

    print(f(1i32));
    print("\n");

    ptrcpy!(f, @b);

    print(f(1i32));
    print("\n");

    0i32
}
