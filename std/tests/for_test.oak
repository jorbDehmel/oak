package!("std");
use_rule!("std");

let main() -> i32
{
    for (let i: i32 = 0; i < 10; ++i)
    {
        print(i);
        endl();
    }

    for (let i = 0; i < 10; ++i)
    {
        print(i);
        endl();
    }

    let i: i32;
    for (i = 0i32; i < 10; ++i)
    {
        print(i);
        endl();
    }

    0
}
