package!("std");

let main() -> i32
{
    let x: i32;
    x = 0;

    print(x);
    endl();

    ++x;

    print(x);
    endl();

    --x;

    print(x);
    endl();

    if (!false)
    {
        print("Hello!\n");
    }

    0
}
