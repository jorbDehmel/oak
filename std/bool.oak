// Ensure that this will work with other dialects
tag!("no_dialect");

new_rule!("and", "and", "&&");
new_rule!("or", "or", "||");

new_rule!("not", "not $~ $*/[$)$] $>a )", "! ( $a )");

bundle_rule!(
    "bool",
    "and",
    "or",
    "not"
);
