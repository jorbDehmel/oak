package!("std");
use_rule!("std");

include!("std/opt.oak");
include!("std/unit.oak");

let print<t>(what: opt<t>) -> void
{
    match (what)
    {
        case some(data)
        {
            print(^data);
            print("\n");
        }
        default
        {
            print("None\n");
        }
    }
}

let main() -> i32
{
    let inst: opt<i32>;
    
    print<i32>(_: opt<i32>);

    print(inst);

    inst.wrap_some(123);

    print(inst);

    0
}
