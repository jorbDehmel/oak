/*
Tests serialization of stl::vec<t> structures.
*/

package!("std", "cereal");
include!("stl/vec.oak");
include!("cereal/stl_vec.oak");
use_rule!("std");

let main() -> i32
{
    where stl::vec<i32> is cereal::serializable;
    let v: stl::vec<i32>;

    for (let i = 0i32; i < 32i32; ++i)
    {
        v.push_back(i);
        print(i);
        endl();
    }

    let pickled: string = v.serialize();

    print(pickled);
    endl();

    0i32
}
